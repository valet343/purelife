{{ header }}
<div id="information-information" class="container">
  <div class="columns">
    {{ column_left }}
    <div id="content" class="{{ class }}">
      <ul class="breadcrumb">
        {% for breadcrumb in breadcrumbs %}
        <li><a href="{{ breadcrumb.href }}">{{ breadcrumb.text }}</a></li>
        {% endfor %}
      </ul>
      <h1>{{ heading_title }}</h1>
      {{ content_top }}
      <div class="description_tags">{{ description }}</div>
      {{ content_bottom }}
    </div>
    {{ column_right }}
    </div>
</div>
{{ footer }}